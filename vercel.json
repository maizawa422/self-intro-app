{
  "version": 2,
  "builds": [
    {
      "src": "api/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/server.js"
    },
    {
      "src": "/user",
      "dest": "/public/user.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
